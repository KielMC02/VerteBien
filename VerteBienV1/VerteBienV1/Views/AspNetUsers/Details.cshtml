@model VerteBienV1.Models.AspNetUsers

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    @{
        List<String> _list = ViewData["imagenes_s"] as List<String>;
    }
    <div class="carousel">
        <div class="carousel__contenedor">
            <button aria-label="Anterior" class="carousel__anterior">
                <i class="fas fa-chevron-left"></i>
            </button>

            <div class="carousel__lista">
                @if (_list != null)
                {
                    foreach (var item in _list)
                    {
                        <div class="carousel__elemento">
                            <a href="~/Imagenes_local/" data-lightbox="Sgaleria" style="width:100%; height:100%;">
                                <img src="~/Imagenes_local/@item" alt="" style=" height: 350px;">
                            </a>
                        </div>
                    }
                }
            </div>
            <button aria-label="Siguiente" class="carousel__siguiente">
                <i class="fas fa-chevron-right"></i>
            </button>

            <div role="tablist" class="carousel__indicadores"></div>
        </div>
    </div>
