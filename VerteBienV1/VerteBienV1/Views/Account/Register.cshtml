                                        <i class="formulario__validacion-estado control-form-feedback fas fa-times-circle"></i>
                                        @Html.TextBoxFor(m => m.Email, new { @class = "form-control control-form control-form", id = "Email", name = "Email", placeholder = "Correo@dominio.com" })

                                    </div>
                                    @Html.ValidationMessageFor(m => m.Email, "", new { @class = "text-danger" })
