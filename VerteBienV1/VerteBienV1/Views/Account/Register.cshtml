            /* Agregamos una animación simple que cambia la opacidad del ícono del ojo */
            .fa-eye {
                transition: opacity 0.3s ease;
            }

            .fa-eye-slash {
                transition: opacity 0.3s ease;
            }

            /* Cambiamos la opacidad del ícono para que aparezca más transparente cuando la contraseña está oculta */
            .fa-eye-slash {
                opacity: 0.5;
            }
            #Email-error {
                display: none;
            }

