<script>
    function mostrarOcultarPassword(inputId, eyeIconId) {
        let input = document.getElementById(inputId);
        let ojoIcono = document.getElementById(eyeIconId);
        if (input.type === "password") {
            input.type = "text";
            ojoIcono.classList.remove('fa-eye');
            ojoIcono.classList.add('fa-eye-slash');
        } else {
            input.type = "password";
            ojoIcono.classList.remove('fa-eye-slash');
            ojoIcono.classList.add('fa-eye');
        }
    }

    document.addEventListener('DOMContentLoaded', function () {
        let txtLoginError = document.getElementById('txt-loginerror');
        let emailInput = document.getElementById('grupo__Email');

        if (txtLoginError.style.display !== 'none') {
            emailInput.classList.add('formulario__grupo-incorrecto');
            setTimeout(function () {
                txtLoginError.style.display = 'none';
                emailInput.classList.remove('formulario__grupo-incorrecto');
            }, 10000);
        }
    });



</script>


